// Place your settings in this file to overwrite the default settings
{
      "editor.minimap.enabled": true,
      "editor.multiCursorModifier": "ctrlCmd",
      "files.associations": {
            "*.mcfunction": "mcfunction",
            "*.bbplugin": "javascript"
      },
      "editor.renderWhitespace": "all",
      "breadcrumbs.enabled": false,
      "datapackLanguageServer.lint.blockStateSortKeys": true,
      "datapackLanguageServer.lint.enableFormatting": true,
      "datapackLanguageServer.lint.nameOfObjectives": "snake_case",
      "datapackLanguageServer.lint.nameOfSnbtCompoundTagKeys": "whatever",
      "datapackLanguageServer.lint.nameOfTags": "snake_case",
      "datapackLanguageServer.lint.nameOfTeams": "snake_case",
      "datapackLanguageServer.lint.omitDefaultNamespace": true,
      "datapackLanguageServer.lint.snbtAppendSpaceAfterColon": false,
      "datapackLanguageServer.lint.snbtAppendSpaceAfterComma": false,
      "datapackLanguageServer.lint.snbtAppendSpaceAfterSemicolon": false,
      "datapackLanguageServer.lint.snbtOmitDoubleSuffix": true,
      "datapackLanguageServer.lint.entitySelectorKeyOrder": [
            "type",
            "gamemode",
            "gamemodeNeg",
            "level",
            "team",
            "teamNeg",
            "typeNeg",
            "tag",
            "tagNeg",
            "name",
            "nameNeg",
            "predicate",
            "predicateNeg",
            "scores",
            "advancements",
            "nbt",
            "nbtNeg",
            "x",
            "y",
            "z",
            "dx",
            "dy",
            "dz",
            "distance",
            "x_rotation",
            "y_rotation",
            "limit",
            "sort"
      ]

}