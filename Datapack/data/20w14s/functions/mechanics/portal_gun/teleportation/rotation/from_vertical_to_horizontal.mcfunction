# Apply rotation
execute if score portal_out_direction 20w14s.prtl_tp matches 0 run teleport @s ~ ~ ~ 180 ~
execute if score portal_out_direction 20w14s.prtl_tp matches 1 run teleport @s ~ ~ ~ 270 ~
execute if score portal_out_direction 20w14s.prtl_tp matches 2 run teleport @s ~ ~ ~ 0 ~
execute if score portal_out_direction 20w14s.prtl_tp matches 3 run teleport @s ~ ~ ~ 90 ~